{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "array-bracket-spacing": "off",
      "brace-style": "off",
      "space-unary-ops": "off",
      "prettier/prettier": "error"
    }
}
