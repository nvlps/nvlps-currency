{
  "name": "nvlps-currency",
  "version": "1.0.0",
  "description": "Javascript Currency Library for nvlps",
  "private": true,
  "main": "src/nvlps-currency.js",
  "scripts": {
    "build": "babel src --out-dir ./dist --source-maps",
    "build-ccy": "pipenv run python scripts/gen_ccy_data.py ./src/lib/ccy-data.js && prettier --write ./src/lib/ccy-data.js",
    "build-l10n": "pipenv run python scripts/gen_ccy_l10n.py --posix ./src/lib/posix.js --es6 ./src/locales/es6 --web ./src/locales/web && prettier --write ./src/lib/posix.js && prettier --write ./src/locales/**/*.js",
    "lint": "eslint ./src && prettier --check ./src/**/*.js",
    "pylint": "flake8",
    "test": "mocha --require @babel/register test/ --check-leaks --globals navigator,process",
    "coverage": "nyc npm test && nyc report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nvlps/nvlps-currency.git"
  },
  "bugs": {
    "url": "https://github.com/nvlps/nvlps-currency/issues"
  },
  "homepage": "https://github.com/nvlps/nvlps-currency#readme",
  "keywords": [
    "currency",
    "money",
    "parse",
    "format",
    "validate",
    "i18n",
    "l10n"
  ],
  "author": "Jonathan Krauss <jkrauss@asymworks.com>",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "babel-plugin-inline-json-import": "^0.3.2",
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "glob": "^7.1.6",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "prettier": "~1.19.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "decimal.js-light": "^2.5.0"
  }
}
